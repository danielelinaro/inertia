{
    "generator_IDs": [1],
    "data_dirs": ["data/two-area/var_H_G{}/two-area_D=2_DZA=60.0"],
    "var_names": ["omegael_bus7", "Pe_bus7"],
    "max_block_size": 1000,
    "model_arch": {
	"N_dims": 1,
	"N_units": {
	    "conv": [16, 32, 64],
	    "pooling": [4, 4, 4],
	    "dense": [64]
	},
	"preproc_activation": null,
	"activation_loc": "after_conv",
	"kernel_size": [5, 5, 5],
	"dropout_coeff": 0.0
    },
    "loss_function": {
	"name": "MAE"
    },
    "optimizer": {
	"name": "Adam",
	"Adam": {
	    "learning_rate": 1e-4
