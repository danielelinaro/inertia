	    "max_learning_rate": 5e-3,
	    "factor": 10.0
	}
    },
    "callbacks": {
	"names": ["early_stopping"],
	"early_stopping": {
	    "monitor": "val_loss",
	    "patience": 500,
	    "mode": "min"
	},
	"reduce_on_plateau": {
	    "monitor": "val_loss",
	    "mode": "min",
	    "factor": 0.5,
	    "patience": 50,
	    "cooldown": 5,
	    "min_learning_rate": 1e-7
	}
    },
    "N_epochs": 1000,
    "batch_size": 128
