{
    "area_IDs": [1],
    "data_dirs": ["data/IEEE39/PowerFactory/var_H_area_{}"],
    "area_IDs_to_learn_inertia": [1],
    "generators_areas_map": [
	["G 02", "G 03", "G 10"],
	["G 04", "G 05", "G 06", "G 07"],
	["G 08", "G 09"],
	["G 01"]
    ],
    "generators_Pnom": {
	"G 01": 10000e6, "G 02": 700e6, "G 03": 800e6, "G 04":  800e6, "G 05":  300e6,
	"G 06":   800e6, "G 07": 700e6, "G 08": 700e6, "G 09": 1000e6, "G 10": 1000e6
    },
    "area_measure": "momentum",
    "var_names": ["Pe_bus3",  "Vd_bus3",  "Vq_bus3",
                  "Pe_bus14", "Vd_bus14", "Vq_bus14",
                  "Pe_bus17", "Vd_bus17", "Vq_bus17"],
    "max_block_size": 500,
    "normalization": "training_set",
    "comet_experiment_tags": ["PowerFactory"],
    "model_arch": {
	"N_dims": 1,
	"N_units": {
	    "conv": [16, 32, 64],
	    "pooling": [4, 4, 4],
	    "dense": [64]
	},
	"preproc_activation": null,
	"activation_loc": "after_conv",
	"kernel_size": [5, 5, 5],
	"dropout_coeff": 0.0
    },
    "loss_function": {
